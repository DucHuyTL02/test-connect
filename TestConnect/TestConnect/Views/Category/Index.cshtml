@model IEnumerable<TestConnect.Models.Category>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDCate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameCate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IDCate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameCate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { IDCate=item.IDCate }) |
            @Html.ActionLink("Details", "Details", new { IDCate=item.IDCate }) |
            @Html.ActionLink("Delete", "Delete", new { IDCate=item.IDCate })
        </td>
    </tr>
}

</table>

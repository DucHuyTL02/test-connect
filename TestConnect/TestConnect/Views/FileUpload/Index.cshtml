@model TestConnect.Models.FileUploadImage

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("UpLoadFile", "FileUpLoad", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <p> @Html.EditorFor(m=>m.fileImage,new { htmlAttributes = new {@type="file"}})</p>
                <p>
                    <input type="submit" value="Upload Image " class="btn btn-danger" />
                 </p>
                 <p>@ViewBag.fileStatus</p>
                 <p>@Session["fileImage"]</p>
                 <p>
                     @Html.ActionLink("Back to Create Product ","Create","Product")
                 </p>
            }

